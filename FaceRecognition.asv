function [foundIndex, distanceDifference] = FaceRecognition(inFace, Weights)
index = 0;
E = 9000;
for i = 1:dsLength
    
    temp = norm(inFace - Weights(:, i));
    
    if temp < E
        E = temp
        index = i;
    end
    
end


threshold = 1550000000000;
% Might need to be changed depending on other images of the same person
if E >= threshold
        index = 0;
end

foundIndex = index;
distanceDifference = E;

end